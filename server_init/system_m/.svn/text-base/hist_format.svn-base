export PROMPT_COMMAND='{ datetime=$(date "+%Y-%m-%d %T");zone='Henan';describtion='Caiji';user_info=$(who am i |awk "{print \$1\" # \"\$5}");server_ip=$(/sbin/ifconfig| grep inet| grep -v "inet6"|grep -v "127.0.0.1"| awk -F":" "{print \$2}"| awk "{print \$1}");uid=$(id|awk "{print \$1}");command=$(history 1 | { read x y; echo $y; }); logger -p local1.notice "$datetime # $zone # $describtion # $user_info # $server_ip # $uid # $command"; } >>$HISTORY_FILE'
